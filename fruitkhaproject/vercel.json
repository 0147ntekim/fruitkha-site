{
    "build": [{
        "src": "fruitkhaProject/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdasize": "15mb",
            "runtime": "python3.12.7"
        }
    }],

    "routes": [
        {
            "src": "/(.*)",
            "dest": "fruitkhaProject/wsgi.py"
        }
    ]
}